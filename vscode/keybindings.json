        "when": "terminalProcessSupported"
    },
    */
    {
        "key": "ctrl+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
    },
    {
        "key": "alt+win+right",
        "command": "workbench.action.positionPanelRight",
        "when": "panelFocus"
    },
    {
        "key": "ctrl+b",
        "command": "python.execInTerminal",
        "when": "resourceExtname == .py"
    },
    {
        "key": "escape",
        "command": "workbench.action.closePanel",
        "when": "panelFocus"
    },
    // {
    //     "key": "alt+2",
    //     "command": "-workbench.action.openEditorAtIndex2"
    // },
    {
        "key": "alt+2",
        "command": "workbench.actions.view.problems",
        "when": "workbench.panel.markers.view.active"
    },
    {
        "key": "ctrl+shift+m",
        "command": "-workbench.actions.view.problems",
        "when": "workbench.panel.markers.view.active"
    },
    {
        "key": "ctrl+b",
        "command": "markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    /*
    {
        "key": "ctrl+k v",
        "command": "-markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    */
    {
        "key": "alt+win+down",
        "command": "workbench.action.positionPanelBottom",
        "when": "panelFocus"
    },
    {
        "key": "alt+win+left",
        "command": "workbench.action.positionPanelLeft",
        "when": "panelFocus"
    },
    /*
    {
        "key": "ctrl+shift+alt+m ctrl+shift+alt+m",
        "command": "editor.action.toggleTabFocusMode"
    },
    {
        "key": "ctrl+m",
        "command": "-editor.action.toggleTabFocusMode"
    },
    */
    {
        "key": "ctrl+b",
        "command": "java.debug.runJavaFile",
        "when": "resourceExtname == .java"
    },
    {
        "key": "ctrl+b",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cd '${fileDirname}'\u000Ddel ${fileBasenameNoExtension}.exe\u000Dcsc '${file}'\u000Dcls\u000D.\\${fileBasenameNoExtension}.exe\u000D",
        },
        "when": "resourceExtname == .cs"
    },
    {
        "key": "ctrl+b",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cd '${fileDirname}'\u000Dnode ${fileBasename}\u000D", /*cd '${fileDirname}'\u000Ddel ${fileBasenameNoExtension}.exe\u000Dcsc '${file}'\u000Dcls\u000D.\\${fileBasenameNoExtension}.exe\u000D*/
        },
        "when": "resourceExtname == .js"
    },
