        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "openTabRenamer",
            "keys": "ctrl+f2"
        },
        {
            "command": 
            {
                "action": "globalSummon",
                "desktop": "toCurrent",
                "monitor": "any",
                "name": "_quake",
                "toggleVisibility": true
            },
            "keys": "win+`"
        }
    ],
    "alwaysOnTop": false,
    "copyFormatting": "none",
    "copyOnSelect": false,
